{
  "name": "AvatarShoppingFramework",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Shared": {
        "$path": "src/shared"
      },
      "ClientModules" :  {
        "$path": "src/client_modules"
      }
    },

    "ServerScriptService": {
      "Server": {
        "$path": "src/server"
      },
      "ServerModules" : {
        "$path" : "src/server_modules"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": {
          "$path": "src/client"
        }
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}